[boot]
image_path = /media/mmcsd-0-0/{{release_name}}/erts-{{erts_vsn}}/bin/beam.bin

[erlang]
args = erl.rtems -- -mode embedded -home . -pa . -root {{release_name}} -boot {{release_name}}/releases/{{release_version}}/{{release_name}} -internal_epmd epmd_sup -kernel inetrc "./erl_inetrc" -sname {{release_name}} -setcookie MyCookie

[network]
wlan=enable
ip_self=dhcp
wlan_ip_netmask=255.255.0.0
wlan_mode=adhoc
wlan_adhocname=edge
wlan_channel=6
hostname=my_grisp_board_2
wpa=wpa_supplicant.conf